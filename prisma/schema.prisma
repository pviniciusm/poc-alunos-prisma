generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id String @id @default(uuid())
  nome String
  email String @unique
  idade Int
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}